server {
 listen ${PORT};

 location /services/RunYAMTL_m2m_groovy {
   proxy_pass http://127.0.0.1:8001/;
 }

  location / {
      root   /usr/share/nginx/html;
      index  index.html index.htm;
      
      add_header 'Access-Control-Allow-Origin' '*';
      
      autoindex on;
      autoindex_format html;
  }


}
